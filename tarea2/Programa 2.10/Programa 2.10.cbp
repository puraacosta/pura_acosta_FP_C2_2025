#include <stdio.h>

/* Par, impar o nulo.
   Realizado por: Pura Acosta
*/

int main(void)
{
    int NUM;

    printf("Ingrese el numero: ");
    scanf("%d", &NUM);

    if (NUM == 0)
        printf("\nNulo\n");
    else if (NUM % 2 == 0)
        printf("\nPar\n");
    else
        printf("\nImpar\n");

    return 0;
}
